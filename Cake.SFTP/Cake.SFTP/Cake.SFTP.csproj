<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>Jeroen Vandezande</Authors>
        <Copyright>Copyright Â© Jeroen Vandezande 2023</Copyright>
        <PackageProjectUrl>https://github.com/JeroenVandezande/Cake.SFTP</PackageProjectUrl>
        <RepositoryUrl>https://github.com/JeroenVandezande/Cake.SFTP</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>Cake Script Build SFTP</PackageTags>
        <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Cake.Core" Version="3.1.0" />
      <PackageReference Include="SSH.NET" Version="2020.0.2" />
    </ItemGroup>

</Project>
